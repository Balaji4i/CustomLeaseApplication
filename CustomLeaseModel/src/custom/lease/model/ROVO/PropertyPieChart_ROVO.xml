<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PropertyPieChart_ROVO"
  Version="11.1.1.66.73"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Variable
    Name="BV_PROPERTY_ID"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT COUNT(*) AS COUNT,
  V.LOOKUP_VALUE_NAME_DISP
FROM XXPL_PROPERTY_UNITS U
INNER JOIN XXFND_LOOKUP_V V
ON V.LOOKUP_VALUE_NAME = U.STATUS AND PROPERTY_ID=:BV_PROPERTY_ID
GROUP BY V.LOOKUP_VALUE_NAME_DISP]]>
  </SQLQuery>
  <ViewAttribute
    Name="Count1"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="COUNT1"
    Expression="COUNT"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="LookupValueNameDisp"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="LOOKUP_VALUE_NAME_DISP"
    Expression="LOOKUP_VALUE_NAME_DISP"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
  </ViewAttribute>
</ViewObject>
